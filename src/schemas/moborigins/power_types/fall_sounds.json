{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://moborigins.ultrusmods.me/en/latest/power_types/fall_sounds/",
	"type": "object",
	"properties": {
		"type": {
			"description": "### ⚠️ Requires [Mob Origins](https://moborigins.ultrusmods.me/en/latest/) to be installed! ⚠️\n\n",
			"markdownDescription": "### ⚠️ Requires [Mob Origins](https://moborigins.ultrusmods.me/en/latest/) to be installed! ⚠️\n\n"
		},
		"distance": {
			"type": "integer",
			"default": 4
		},
		"small_fall_sound": {
			"default": "minecraft:entity.generic.small_fall",
			"$ref": "$ref(apoli:types/autofill_helpers/sound_identifier)"
		},
		"big_fall_sound": {
			"default": "minecraft:entity.generic.big_fall",
			"$ref": "$ref(apoli:types/autofill_helpers/sound_identifier)"
		}
	}
}

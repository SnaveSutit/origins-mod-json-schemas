{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/power_types/status_bar_texture/",
	"type": "object",
	"required": ["texture"],
	"properties": {
		"texture": {
			"$ref": "$ref(apoli:types/identifier)"
		},
		"texture_map": {
			"type": "object",
			"patternProperties": {
				"^(([a-z0-9_\\/\\-\\.\\*]+):)?[a-z0-9_\\/\\-\\.\\*]+$": {
					"$ref": "$ref(apoli:types/identifier)"
				}
			}
		}
	}
}

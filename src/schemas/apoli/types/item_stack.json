{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/data_types/item_stack/",
	"type": "object",
	"required": ["item"],
	"dependencies": {
		"components": { "not": { "required": ["tag"] } },
		"tag": { "not": { "required": ["components"] } }
	},
	"properties": {
		"item": {
			"$ref": "$ref(apoli:types/autofill_helpers/item_identifier)"
		},
		"amount": {
			"type": "integer"
		},
		"tag": {
			"$ref": "$ref(apoli:types/nbt)"
		},
		"$IMPORT": {
			"type": "import_files_as_object_properties",
			"path": "$ref(minecraft:item_components)",
			"output_key": "components",
			"property_name_template": "minecraft:$fileName",
			"schema_structure": {
				"$ref": "$fileRef"
			}
		},
		"components": {}
	}
}
